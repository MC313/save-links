{
    "function": {
        "SaveLinkLambda": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "LinksTable",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        },
        "ListUserLinksLambda": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "LinksTable",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        },
        "DependenciesLayer": {
            "runtimes": [
                {
                    "value": "nodejs",
                    "name": "NodeJS",
                    "layerExecutablePath": "nodejs/node_modules",
                    "cloudTemplateValue": "nodejs12.x"
                }
            ],
            "layerVersionMap": {
                "1": {
                    "permissions": [
                        {
                            "type": "awsAccounts",
                            "accounts": [
                                "420408896210"
                            ]
                        },
                        {
                            "type": "private"
                        }
                    ]
                }
            },
            "providerPlugin": "awscloudformation",
            "service": "LambdaLayer",
            "build": true
        },
        "LinksLockerAuthPostConfirmation": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "ScheduleReminderLambda": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "SendReminderNotificationLambda",
                    "attributes": [
                        "Arn"
                    ]
                },
                {
                    "category": "sns",
                    "resourceName": "ReminderNotificationTopic",
                    "attributes": [
                        "Arn"
                    ]
                },
                {
                    "category": "storage",
                    "resourceName": "LinksTable",
                    "attributes": [
                        "Name",
                        "StreamArn",
                        "Arn"
                    ]
                }
            ]
        },
        "SendReminderNotificationLambda": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "DependenciesLayer",
                    "attributes": [
                        "Arn"
                    ]
                }
            ]
        },
        "NotificationLambda": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": []
        }
    },
    "auth": {
        "userPoolGroups": {
            "service": "Cognito-UserPool-Groups",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "auth",
                    "resourceName": "LinksLockerAuth",
                    "attributes": [
                        "UserPoolId",
                        "AppClientIDWeb",
                        "AppClientID",
                        "IdentityPoolId"
                    ]
                }
            ]
        },
        "LinksLockerAuth": {
            "service": "Cognito",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "LinksLockerAuthPostConfirmation",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                }
            ]
        }
    },
    "api": {
        "LinksLockerAPI": {
            "service": "API Gateway",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "SaveLinkLambda",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "ListUserLinksLambda",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "NotificationLambda",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        }
    },
    "storage": {
        "LinksTable": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        }
    },
    "sns": {
        "ReminderNotificationTopic": {
            "service": "sns",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "api",
                    "resourceName": "LinksLockerAPI",
                    "attributes": [
                        "RootUrl"
                    ]
                }
            ]
        }
    }
}